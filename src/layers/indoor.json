[{
    "id": "indoor-tag",
    "source": "indoor_source",
    "minzoom": 17,
    "visibility": "none",
    "filter": [
        "all", [
            "has",
            "indoor"
        ]
    ],
    "type": "line",
    "paint": {
        "line-opacity": 1,
        "line-color": [
            "case", [
                "==", [
                    "feature-state",
                    "hover"
                ],
                null
            ],
            [
                "match", ["%", ["id"], 4],
                0, "#ea4234",
                1, "#fabc05",
                2, "#34a853",
                "#4286f4"
            ],
            [
                "boolean", [
                    "feature-state",
                    "hover"
                ]
            ],
            "white", [
                "match", ["%", ["id"], 4],
                0, "#ea4234",
                1, "#fabc05",
                2, "#34a853",
                "#4286f4"
            ]
        ],
        "line-width": 5
    }
